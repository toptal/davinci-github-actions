    "@toptal/browserslist-config": "^1.2.0",
    "@toptal/davinci-qa": "^12.1.0",
    "@toptal/davinci-syntax": "^16.2.0",
    "@types/js-yaml": "^4.0.5",
    "@types/marked": "^4.0.3",
